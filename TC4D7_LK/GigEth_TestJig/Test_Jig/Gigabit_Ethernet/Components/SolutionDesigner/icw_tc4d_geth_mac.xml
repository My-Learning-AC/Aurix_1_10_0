<?xml version="1.0" encoding="UTF-8"?>
<!-- Parameters: -->
<!--   PORT: port index, range 0..1 -->
<ifxmlcfg version="1.0.0">
    <checksum>cfd7f0c012bb2dcfe53be0e2339321feae5126690bea312def6098ca285bebc5</checksum>
    <version label="V0.1.0"/>
	<svd file="tc4d_eth.svd"/>
	<disclaimer file="disclaimer.txt"/> 
	<!--moduledesc name="Geth$PORT$ MAC" description="Gigabit Ethernet MAC PORT$PORT$" exporter="ModuleExporter.py" hide="0"/-->
    <header file="tc4d_geth_mac_cfg.h">
        <verticalcontainer label="Geth MAC$PORT$">
			<edit visible="false" define="GETH_PORT$PORT$_MAC_GRAPH_COLOR" svd="0" default="ABD9E4"/>
            <checkbox label="Enable MAC$PORT$" define="GETH.MACEN.MAC$PORT$" default="1" font="Source Sans Pro,15,0,0,100,1,0,0,0,0" color="255;128;64">
                <action event="checked" cmd="setDef" data="GETH.PORT$PORT$_MAC_TOP.enabled=1;GETH_PORT$PORT$_MAC_GRAPH_COLOR=ABD9E4;Geth.Diagram.update=1"/>
                <action event="unchecked" cmd="setDef" data="GETH.PORT$PORT$_MAC_TOP.enabled=0;GETH_PORT$PORT$_MAC_GRAPH_COLOR=D0D0D0;Geth.Diagram.update=1"/>
            </checkbox>
			<groupcontainer define="GETH.PORT$PORT$_MAC_TOP" font="Source Sans Pro,12,0,0,100,0,0,0,0,0">
				<verticalcontainer>
					<groupcontainer label="General Settings" font="Source Sans Pro,12,0,0,64,0,0,0,0,0">
						<horizontalcontainer>
							<formcontainer>
								<doublespinbox visible="false" define="GETH.PORT$PORT$_TRANSMIT_CLK_PERIOD_NS" default="1"/>
								<doublespinbox visible="false" define="GETH.PORT$PORT$_CBS_CLK_PERIOD_NS" default="8"/>
								<doublespinbox visible="false" define="GETH.PORT$PORT$_CBS_BITS_PER_CLK" default="8"/>
								<combo label="Link Speed" define="GETH.PORT$PORT$_MAC_TX_CONFIGURATION.SS" default="value=3">
									<!--item label="10G XGMII" value="0"/-->
									<item label="5G XGMII"   value="5" setdef="GETH.PORT$PORT$_TRANSMIT_CLK_PERIOD_NS=0.2;GETH.PORT$PORT$_CBS_CLK_PERIOD_NS=6.4;GETH.PORT$PORT$_CBS_BITS_PER_CLK=32"/>
									<item label="2.5G XGMII" value="2" setdef="GETH.PORT$PORT$_TRANSMIT_CLK_PERIOD_NS=0.4;GETH.PORT$PORT$_CBS_CLK_PERIOD_NS=12.8;GETH.PORT$PORT$_CBS_BITS_PER_CLK=32"/>
									<!--item label="2.5G GMII" value="6" setdef="GETH.PORT$PORT$_TRANSMIT_CLK_PERIOD_NS=0.4;GETH.PORT$PORT$_CBS_CLK_PERIOD_NS=12.8;GETH.PORT$PORT$_CBS_BITS_PER_CLK=32"/-->
									<item label="1G RGMII"   value="3" setdef="GETH.PORT$PORT$_TRANSMIT_CLK_PERIOD_NS=1;GETH.PORT$PORT$_CBS_CLK_PERIOD_NS=8;GETH.PORT$PORT$_CBS_BITS_PER_CLK=8"/>
									<item label="100M MII"   value="4" setdef="GETH.PORT$PORT$_TRANSMIT_CLK_PERIOD_NS=10;GETH.PORT$PORT$_CBS_CLK_PERIOD_NS=40;GETH.PORT$PORT$_CBS_BITS_PER_CLK=4"/>
									<item label="10M MII"    value="7" setdef="GETH.PORT$PORT$_TRANSMIT_CLK_PERIOD_NS=100;GETH.PORT$PORT$_CBS_CLK_PERIOD_NS=400;GETH.PORT$PORT$_CBS_BITS_PER_CLK=4"/>
								</combo>
								<checkbox label="All ECC Enable" define="GETH.PORT$PORT$_MTL_ECC_CONTROL_ALL" role="inverted" default="1"/>
								<math visible="false" define="GETH.PORT$PORT$_MTL_ECC_CONTROL.MTXED" formula="GETH.PORT$PORT$_MTL_ECC_CONTROL_ALL"/>
								<math visible="false" define="GETH.PORT$PORT$_MTL_ECC_CONTROL.MRXED" formula="GETH.PORT$PORT$_MTL_ECC_CONTROL_ALL"/>
								<math visible="false" define="GETH.PORT$PORT$_MTL_ECC_CONTROL.MGCLED" formula="GETH.PORT$PORT$_MTL_ECC_CONTROL_ALL"/>
								<math visible="false" define="GETH.PORT$PORT$_MTL_ECC_CONTROL.MRXPED" formula="GETH.PORT$PORT$_MTL_ECC_CONTROL_ALL"/>
								<math visible="false" define="GETH.PORT$PORT$_MTL_ECC_CONTROL.DESCED" formula="GETH.PORT$PORT$_MTL_ECC_CONTROL_ALL"/>
							</formcontainer>
						</horizontalcontainer>
					</groupcontainer>
					<tabcontainer>
						<verticalcontainer label="Queues">
							<tabcontainer cond="$PORT$==1" define="GETH.PORT$PORT$_MAC_QUEUE_SELECT" size="-1;500">
								<include PORT="$PORT$" IDX="0" SFRIDX="0" SFR2IDX="2">icw_tc4d_geth_mac_queue_tmpl.xml</include>
								<include PORT="$PORT$" IDX="1" SFRIDX="0" SFR2IDX="2">icw_tc4d_geth_mac_queue_tmpl.xml</include>
								<include PORT="$PORT$" IDX="2" SFRIDX="0" SFR2IDX="2">icw_tc4d_geth_mac_queue_tmpl.xml</include>
								<include PORT="$PORT$" IDX="3" SFRIDX="0" SFR2IDX="2">icw_tc4d_geth_mac_queue_tmpl.xml</include>
								<include PORT="$PORT$" IDX="4" SFRIDX="1" SFR2IDX="3">icw_tc4d_geth_mac_queue_tmpl.xml</include>
								<include PORT="$PORT$" IDX="5" SFRIDX="1" SFR2IDX="3">icw_tc4d_geth_mac_queue_tmpl.xml</include>
								<include PORT="$PORT$" IDX="6" SFRIDX="1" SFR2IDX="3">icw_tc4d_geth_mac_queue_tmpl.xml</include>
								<include PORT="$PORT$" IDX="7" SFRIDX="1" SFR2IDX="3">icw_tc4d_geth_mac_queue_tmpl.xml</include>
							</tabcontainer>
							<tabcontainer define="GETH.PORT$PORT$_MAC_TC_SELECT">
								<include PORT="$PORT$" IDX="0">icw_tc4d_geth_mac_queue_tc04_tmpl.xml</include>
								<include PORT="$PORT$" IDX="1">icw_tc4d_geth_mac_queue_tc04_tmpl.xml</include>
								<include PORT="$PORT$" IDX="2">icw_tc4d_geth_mac_queue_tc04_tmpl.xml</include>
								<include PORT="$PORT$" IDX="3">icw_tc4d_geth_mac_queue_tc04_tmpl.xml</include>
								<include PORT="$PORT$" IDX="4">icw_tc4d_geth_mac_queue_tc04_tmpl.xml</include>
								<include PORT="$PORT$" IDX="5">icw_tc4d_geth_mac_queue_tc57_tmpl.xml</include>
								<include PORT="$PORT$" IDX="6">icw_tc4d_geth_mac_queue_tc57_tmpl.xml</include>
								<include PORT="$PORT$" IDX="7">icw_tc4d_geth_mac_queue_tc57_tmpl.xml</include>
							</tabcontainer>
						</verticalcontainer>
						<verticalcontainer label="Filtering">
							<!--groupcontainer label="Filter Settings"-->
								<horizontalcontainer>
									<groupcontainer label="Filter Settings">
										<verticalcontainer>
											<checkbox label="Flexible Frame Parser" define="GETH.PORT$PORT$_MTL_OPERATION_MODE.FRPE" default="0"/>
											<formcontainer>
												<checkbox label="Receive All" define="GETH.PORT$PORT$_MAC_PACKET_FILTER.RA" default="1">
													<action event="checked" cmd="setDef" data="GETH.PORT$PORT$_MAC_PACKET_FILTER.VTFE.enabled=0"/>
													<action event="unchecked" cmd="setDef" data="GETH.PORT$PORT$_MAC_PACKET_FILTER.VTFE.enabled=1"/>
												</checkbox>
												<checkbox label="Promiscuous Mode" define="GETH.PORT$PORT$_MAC_PACKET_FILTER.PR"/>
												<checkbox label="Source Address filtering" define="GETH.PORT$PORT$_MAC_PACKET_FILTER.SAF">
													<action event="checked" cmd="setDef" data="GETH.PORT$PORT$_MAC_PACKET_FILTER.SAIF.enabled=1"/>
													<action event="unchecked" cmd="setDef" data="GETH.PORT$PORT$_MAC_PACKET_FILTER.SAIF.enabled=0"/>
												</checkbox>
												<math visible="false" define="GETH.PORT$PORT$_MAC_FILTER_Cont_Calc" formula="(GETH.PORT$PORT$_MAC_PACKET_FILTER.PR.^1).*GETH.PORT$PORT$_MAC_PACKET_FILTER.RA">
													<action event="changed" cmd="setDef" data="GETH.PORT$PORT$_MAC_FILTER_Cont.enabled=GETH.PORT$PORT$_MAC_FILTER_Cont_Calc"/>
												</math>
												<checkbox label="Source Address inverse filtering" enabled="false" define="GETH.PORT$PORT$_MAC_PACKET_FILTER.SAIF"/>
												<checkbox label="Destination Address inverse filtering" define="GETH.PORT$PORT$_MAC_PACKET_FILTER.DAIF"/>
												<checkbox label="pass all multicast packets" define="GETH.PORT$PORT$_MAC_PACKET_FILTER.PM"/>
												<checkbox label="block all broadcast packets" define="GETH.PORT$PORT$_MAC_PACKET_FILTER.DBF"/>
												<combo label="Control packets filter" define="GETH.PORT$PORT$_MAC_PACKET_FILTER.PCF">
													<item label="block all"/>
													<item label="block pause packes only"/>
													<item label="forward all (even if address filter fails)"/>
													<item label="forward all (only if address filter pass)"/>
												</combo>
												<checkbox label="Layer3 / Layer4 filtering" define="GETH.PORT$PORT$_MAC_PACKET_FILTER.IPFE" lockon="GETH.PORT$PORT$_MAC_RX_CONFIGURATION.IPC=1"/>
												<checkbox label="drop non-TCP/UDP packets" define="GETH.PORT$PORT$_MAC_PACKET_FILTER.DNTU"/>
												<checkbox label="Packet Duplication (for RxQueue7 only)" define="GETH.PORT$PORT$_MAC_EXTENDED_CONFIGURATION.DDS">
													<action event="checked" cmd="setDef" data="GETH.PORT$PORT$_MAC_ADDRESS0_DCS_CONT.visible=0;GETH.PORT$PORT$_MAC_ADDRESS0_XDCS_CONT1.visible=1;GETH.PORT$PORT$_MAC_ADDRESS0_XDCS_CONT2.visible=1"/>
													<action event="checked" cmd="setDef" data="GETH.PORT$PORT$_MAC_ADDRESS1_DCS_CONT.visible=0;GETH.PORT$PORT$_MAC_ADDRESS1_XDCS_CONT1.visible=1;GETH.PORT$PORT$_MAC_ADDRESS1_XDCS_CONT2.visible=1"/>
													<action event="checked" cmd="setDef" data="GETH.PORT$PORT$_MAC_ADDRESS2_DCS_CONT.visible=0;GETH.PORT$PORT$_MAC_ADDRESS2_XDCS_CONT1.visible=1;GETH.PORT$PORT$_MAC_ADDRESS2_XDCS_CONT2.visible=1"/>
													<action event="checked" cmd="setDef" data="GETH.PORT$PORT$_MAC_ADDRESS3_DCS_CONT.visible=0;GETH.PORT$PORT$_MAC_ADDRESS3_XDCS_CONT1.visible=1;GETH.PORT$PORT$_MAC_ADDRESS3_XDCS_CONT2.visible=1"/>
													<action event="checked" cmd="setDef" data="GETH.PORT$PORT$_MAC_ADDRESS4_DCS_CONT.visible=0;GETH.PORT$PORT$_MAC_ADDRESS4_XDCS_CONT1.visible=1;GETH.PORT$PORT$_MAC_ADDRESS4_XDCS_CONT2.visible=1"/>
													<action event="checked" cmd="setDef" data="GETH.PORT$PORT$_MAC_ADDRESS5_DCS_CONT.visible=0;GETH.PORT$PORT$_MAC_ADDRESS5_XDCS_CONT1.visible=1;GETH.PORT$PORT$_MAC_ADDRESS5_XDCS_CONT2.visible=1"/>
													<action event="checked" cmd="setDef" data="GETH.PORT$PORT$_MAC_ADDRESS6_DCS_CONT.visible=0;GETH.PORT$PORT$_MAC_ADDRESS6_XDCS_CONT1.visible=1;GETH.PORT$PORT$_MAC_ADDRESS6_XDCS_CONT2.visible=1"/>
													<action event="checked" cmd="setDef" data="GETH.PORT$PORT$_MAC_ADDRESS7_DCS_CONT.visible=0;GETH.PORT$PORT$_MAC_ADDRESS7_XDCS_CONT1.visible=1;GETH.PORT$PORT$_MAC_ADDRESS7_XDCS_CONT2.visible=1"/>
													<action event="checked" cmd="setDef" data="GETH.PORT$PORT$_MAC_ADDRESS8_DCS_CONT.visible=0;GETH.PORT$PORT$_MAC_ADDRESS8_XDCS_CONT1.visible=1;GETH.PORT$PORT$_MAC_ADDRESS8_XDCS_CONT2.visible=1"/>
													<action event="checked" cmd="setDef" data="GETH.PORT$PORT$_MAC_ADDRESS9_DCS_CONT.visible=0;GETH.PORT$PORT$_MAC_ADDRESS9_XDCS_CONT1.visible=1;GETH.PORT$PORT$_MAC_ADDRESS9_XDCS_CONT2.visible=1"/>
													<action event="checked" cmd="setDef" data="GETH.PORT$PORT$_MAC_RXQ_CTRL4.UDC.enabled=1"/>
													<action event="unchecked" cmd="setDef" data="GETH.PORT$PORT$_MAC_ADDRESS0_DCS_CONT.visible=1;GETH.PORT$PORT$_MAC_ADDRESS0_XDCS_CONT1.visible=0;GETH.PORT$PORT$_MAC_ADDRESS0_XDCS_CONT2.visible=0"/>
													<action event="unchecked" cmd="setDef" data="GETH.PORT$PORT$_MAC_ADDRESS1_DCS_CONT.visible=1;GETH.PORT$PORT$_MAC_ADDRESS1_XDCS_CONT1.visible=0;GETH.PORT$PORT$_MAC_ADDRESS1_XDCS_CONT2.visible=0"/>
													<action event="unchecked" cmd="setDef" data="GETH.PORT$PORT$_MAC_ADDRESS2_DCS_CONT.visible=1;GETH.PORT$PORT$_MAC_ADDRESS2_XDCS_CONT1.visible=0;GETH.PORT$PORT$_MAC_ADDRESS2_XDCS_CONT2.visible=0"/>
													<action event="unchecked" cmd="setDef" data="GETH.PORT$PORT$_MAC_ADDRESS3_DCS_CONT.visible=1;GETH.PORT$PORT$_MAC_ADDRESS3_XDCS_CONT1.visible=0;GETH.PORT$PORT$_MAC_ADDRESS3_XDCS_CONT2.visible=0"/>
													<action event="unchecked" cmd="setDef" data="GETH.PORT$PORT$_MAC_ADDRESS4_DCS_CONT.visible=1;GETH.PORT$PORT$_MAC_ADDRESS4_XDCS_CONT1.visible=0;GETH.PORT$PORT$_MAC_ADDRESS4_XDCS_CONT2.visible=0"/>
													<action event="unchecked" cmd="setDef" data="GETH.PORT$PORT$_MAC_ADDRESS5_DCS_CONT.visible=1;GETH.PORT$PORT$_MAC_ADDRESS5_XDCS_CONT1.visible=0;GETH.PORT$PORT$_MAC_ADDRESS5_XDCS_CONT2.visible=0"/>
													<action event="unchecked" cmd="setDef" data="GETH.PORT$PORT$_MAC_ADDRESS6_DCS_CONT.visible=1;GETH.PORT$PORT$_MAC_ADDRESS6_XDCS_CONT1.visible=0;GETH.PORT$PORT$_MAC_ADDRESS6_XDCS_CONT2.visible=0"/>
													<action event="unchecked" cmd="setDef" data="GETH.PORT$PORT$_MAC_ADDRESS7_DCS_CONT.visible=1;GETH.PORT$PORT$_MAC_ADDRESS7_XDCS_CONT1.visible=0;GETH.PORT$PORT$_MAC_ADDRESS7_XDCS_CONT2.visible=0"/>
													<action event="unchecked" cmd="setDef" data="GETH.PORT$PORT$_MAC_ADDRESS8_DCS_CONT.visible=1;GETH.PORT$PORT$_MAC_ADDRESS8_XDCS_CONT1.visible=0;GETH.PORT$PORT$_MAC_ADDRESS8_XDCS_CONT2.visible=0"/>
													<action event="unchecked" cmd="setDef" data="GETH.PORT$PORT$_MAC_ADDRESS9_DCS_CONT.visible=1;GETH.PORT$PORT$_MAC_ADDRESS9_XDCS_CONT1.visible=0;GETH.PORT$PORT$_MAC_ADDRESS9_XDCS_CONT2.visible=0"/>
													<action event="unchecked" cmd="setDef" data="GETH.PORT$PORT$_MAC_RXQ_CTRL4.UDC.enabled=0"/>
												</checkbox>
												<checkbox enabled="false" label="Unicast Packet duplication" define="GETH.PORT$PORT$_MAC_RXQ_CTRL4.UDC"/>
											</formcontainer>
										</verticalcontainer>
									</groupcontainer>
									<verticalcontainer>
										<groupcontainer label="Filter Pass">
											<!--verticalcontainer-->
												<formcontainer>
													<combo label="Untagged Packet Queue" define="GETH.PORT$PORT$_MAC_RXQ_CTRL1.UPQ">
														<item label="Queue0"/>
														<item label="Queue1"/>
														<item label="Queue2"/>
														<item label="Queue3"/>
														<item label="Queue4"/>
														<item label="Queue5"/>
														<item label="Queue6"/>
														<item label="Queue7"/>
													</combo>
													<combo label="Frame Preemption Residue Queue" define="GETH.PORT$PORT$_MAC_RXQ_CTRL1.RQ" default="1">
														<item label="Queue1" value="1"/>
														<item label="Queue2" value="2"/>
														<item label="Queue3" value="3"/>
														<item label="Queue4" value="4"/>
														<item label="Queue5" value="5"/>
														<item label="Queue6" value="6"/>
														<item label="Queue7" value="7"/>
													</combo>
													<horizontalcontainer label="Multicast/Broadcast Frame" stretch="2">
														<radio define="GETH.PORT$PORT$_MAC_RXQ_CTRL1.MCBCQEN">
															<radiobutton label="drop" setdef="GETH.PORT$PORT$_MAC_RXQ_CTRL1.MCBCQ.enabled=0;GETH.PORT$PORT$_MAC_RXQ_CTRL1.PMCBCQ.enabled=0"/>
															<radiobutton label="accept" setdef="GETH.PORT$PORT$_MAC_RXQ_CTRL1.MCBCQ.enabled=1;GETH.PORT$PORT$_MAC_RXQ_CTRL1.PMCBCQ.enabled=1"/>
														</radio>
													</horizontalcontainer>
													<combo label="Multicast or Broadcast Queue" define="GETH.PORT$PORT$_MAC_RXQ_CTRL1.MCBCQ" enabled="false">
														<item label="Queue0"/>
														<item label="Queue1"/>
														<item label="Queue2"/>
														<item label="Queue3"/>
														<item label="Queue4"/>
														<item label="Queue5"/>
														<item label="Queue6"/>
														<item label="Queue7"/>
													</combo>
													<combo label="Multicast or Broadcast Preemption Queue" define="GETH.PORT$PORT$_MAC_RXQ_CTRL1.PMCBCQ" enabled="false">
														<item label="Queue0"/>
														<item label="Queue1"/>
														<item label="Queue2"/>
														<item label="Queue3"/>
														<item label="Queue4"/>
														<item label="Queue5"/>
														<item label="Queue6"/>
														<item label="Queue7"/>
													</combo>
													<combo label="PTP Packets Queue" define="GETH.PORT$PORT$_MAC_RXQ_CTRL1.PTPQ">
														<item label="Queue0"/>
														<item label="Queue1"/>
														<item label="Queue2"/>
														<item label="Queue3"/>
														<item label="Queue4"/>
														<item label="Queue5"/>
														<item label="Queue6"/>
														<item label="Queue7"/>
													</combo>
													<horizontalcontainer label="AV Control Frame" stretch="2">
														<radio define="GETH.PORT$PORT$_MAC_RXQ_CTRL1.TACPQEN">
															<radiobutton label="drop" setdef="GETH.PORT$PORT$_MAC_RXQ_CTRL1.AVCPQ.enabled=0"/>
															<radiobutton label="accept" setdef="GETH.PORT$PORT$_MAC_RXQ_CTRL1.AVCPQ.enabled=1"/>
														</radio>
													</horizontalcontainer>
													<combo label="AV Control Queue" define="GETH.PORT$PORT$_MAC_RXQ_CTRL1.AVCPQ">
														<item label="Queue0"/>
														<item label="Queue1"/>
														<item label="Queue2"/>
														<item label="Queue3"/>
														<item label="Queue4"/>
														<item label="Queue5"/>
														<item label="Queue6"/>
														<item label="Queue7"/>
													</combo>
													<combo label="Tagged PTP over Ethernet Packets Queuing Control" define="GETH.PORT$PORT$_MAC_RXQ_CTRL1.TPQC">
														<item label="routed as generic VLAN Tagged packet"/>
														<item label="routed to Rx Queue specified by PTPQ field"/>
														<item label="routed to only AV enabled Rx Queues"/>
													</combo>
												</formcontainer>
											<!--/verticalcontainer-->
										</groupcontainer>
										<groupcontainer label="Filter Fail">
											<verticalcontainer>
												<horizontalcontainer enabled="false" define="GETH.PORT$PORT$_MAC_FILTER_Cont">
													<formcontainer>
														<horizontalcontainer label="Unicast Frame" stretch="2">
															<radio define="GETH.PORT$PORT$_MAC_RXQ_CTRL4.UFFQE">
																<radiobutton label="untagged Packet Queue" setdef="GETH.PORT$PORT$_MAC_RXQ_CTRL4.UFFQ.enabled=0"/>
																<radiobutton label="Unicast Queue" setdef="GETH.PORT$PORT$_MAC_RXQ_CTRL4.UFFQ.enabled=1"/>
															</radio>
														</horizontalcontainer>
														<combo label="Unicast Queue" define="GETH.PORT$PORT$_MAC_RXQ_CTRL4.UFFQ" enabled="false">
															<item label="Queue0"/>
															<item label="Queue1"/>
															<item label="Queue2"/>
															<item label="Queue3"/>
															<item label="Queue4"/>
															<item label="Queue5"/>
															<item label="Queue6"/>
															<item label="Queue7"/>
														</combo>
														<horizontalcontainer label="Multicast Frame" stretch="2">
															<radio define="GETH.PORT$PORT$_MAC_RXQ_CTRL4.MFFQE">
																<radiobutton label="drop" setdef="GETH.PORT$PORT$_MAC_RXQ_CTRL4.MFFQ.enabled=0"/>
																<radiobutton label="Multicast Queue" setdef="GETH.PORT$PORT$_MAC_RXQ_CTRL4.MFFQ.enabled=1"/>
															</radio>
														</horizontalcontainer>
														<combo label="Multicast Queue" define="GETH.PORT$PORT$_MAC_RXQ_CTRL4.MFFQ" enabled="false">
															<item label="Queue0"/>
															<item label="Queue1"/>
															<item label="Queue2"/>
															<item label="Queue3"/>
															<item label="Queue4"/>
															<item label="Queue5"/>
															<item label="Queue6"/>
															<item label="Queue7"/>
														</combo>
													</formcontainer>
												</horizontalcontainer>
												<formcontainer>
													<combo label="VLAN Filter Frame Fail" define="GETH.PORT$PORT$_MAC_PACKET_FILTER.VTFE">
														<item label="drop frame" value="1" setdef="GETH.PORT$PORT$_MAC_RXQ_CTRL4_VFFQE_CONT.enabled=0;GETH.PORT$PORT$_MAC_RXQ_CTRL4.VFFQ.enabled=0"/>
														<item label="further proccessing" value="0" setdef="GETH.PORT$PORT$_MAC_RXQ_CTRL4_VFFQE_CONT.enabled=1;GETH.PORT$PORT$_MAC_RXQ_CTRL4.VFFQ.enabled=GETH.PORT$PORT$_MAC_RXQ_CTRL4.VFFQE"/>
													</combo>
													<math visible="false" define="GETH.PORT$PORT$_MAC_RXQ_CTRL4_VFFQE_CONT_CALC" formula="GETH.PORT$PORT$_MAC_PACKET_FILTER.RA.+(GETH.PORT$PORT$_MAC_PACKET_FILTER.VTFE.^1)">
														<action event="changed" cmd="setDef" data="GETH.PORT$PORT$_MAC_RXQ_CTRL4_VFFQE_CONT.enabled=GETH.PORT$PORT$_MAC_RXQ_CTRL4_VFFQE_CONT_CALC"/>
													</math>
													<horizontalcontainer label="VLAN Tagged Frame" define="GETH.PORT$PORT$_MAC_RXQ_CTRL4_VFFQE_CONT" stretch="2">
														<radio define="GETH.PORT$PORT$_MAC_RXQ_CTRL4.VFFQE">
															<radiobutton label="Prio based routing" setdef="GETH.PORT$PORT$_MAC_RXQ_CTRL4.VFFQ.enabled=0"/>
															<radiobutton label="VLAN Filter Queue " setdef="GETH.PORT$PORT$_MAC_RXQ_CTRL4.VFFQ.enabled=1"/>
														</radio>
													</horizontalcontainer>
													<combo label="VLAN Filter Queue" define="GETH.PORT$PORT$_MAC_RXQ_CTRL4.VFFQ" enabled="false">
														<item label="Queue0"/>
														<item label="Queue1"/>
														<item label="Queue2"/>
														<item label="Queue3"/>
														<item label="Queue4"/>
														<item label="Queue5"/>
														<item label="Queue6"/>
														<item label="Queue7"/>
													</combo>
												</formcontainer>
												<formcontainer>
													<horizontalcontainer label="Policy Counter">
														<verticalcontainer size="30;-1">
															<checkbox define="GETH.PORT$PORT$_MAC_FPCSEL_INDREG0.PCEN">
																<action event="checked" cmd="setDef" data="GETH.PORT$PORT$_MAC_FPCSEL_INDREG0_CONT.enabled=1"/>
																<action event="unchecked" cmd="setDef" data="GETH.PORT$PORT$_MAC_FPCSEL_INDREG0_CONT.enabled=0"/>
															</checkbox>
														</verticalcontainer>
														<verticalcontainer enabled="false" define="GETH.PORT$PORT$_MAC_FPCSEL_INDREG0_CONT">
															<spinbox define="GETH.PORT$PORT$_MAC_FPCSEL_INDREG0.PCNUM" range="0;15"/>
														</verticalcontainer>
													</horizontalcontainer>
												</formcontainer>
											</verticalcontainer>
										</groupcontainer>
